    <!-- https://archive.org/services/purl/domain/otd#associatedGender -->

    <owl:ObjectProperty rdf:about="https://archive.org/services/purl/domain/otd#associatedGender">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://archive.org/services/purl/domain/otd#Costume"/>
                    <rdf:Description rdf:about="https://archive.org/services/purl/domain/otd#Dance_instruments"/>
