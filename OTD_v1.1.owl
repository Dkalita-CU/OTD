                    <rdf:Description rdf:about="https://archive.org/services/purl/domain/otd#Ornaments"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://archive.org/services/purl/domain/otd#Gender"/>
        <rdfs:comment xml:lang="en">Different ornaments, costumes, instruments used in the traditional dances performance has a specific gender by which that item will be used in that particular dance. This property connects a associated gender of such items.</rdfs:comment>
    </owl:ObjectProperty>
    


